{
  "name": "@oneserv/phpstan-ruleset",
  "version": "1.3.0",
  "description": "An opinionated ruleset for PHPStan to enforce Oneserv Coding Standards.",
  "scripts": {
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneserv/phpstan-ruleset.git"
  },
  "author": {
    "name": "Timon Heuser",
    "email": "heuser@bueromoebel-experte.de",
    "url": "https://github.com/oneserv"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oneserv/phpstan-ruleset/issues"
  },
  "homepage": "https://github.com/oneserv/phpstan-ruleset#readme",
  "devDependencies": {
    "standard-version": "^9.3.1"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "change",
        "section": "Changes"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "depr",
        "section": "Deprecations"
      },
      {
        "type": "remove",
        "section": "Removals"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "dev",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ]
  }
}
